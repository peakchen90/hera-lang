pub type Vec = []char

enum Color {
    Red,
    Green,
}

pub interface Person {
    say() -> str
}

pub struct Man {
    name: str
}

pub struct Woman {
    nick: str
}

impl (Person) Man {
    pub fn say() {
        return "Man: " + self.name
    }
}

impl (a.Person) b.Woman {
    pub fn say() {
        return "Woman: " + self.nick
    }
}

impl c.Woman {
    pub fn say() {
        return "Woman: " + self.nick
    }
}

struct OldMan <- Man {
    age: num
}

struct Student <- a.Man, b.Woman {
    scores: []num
}