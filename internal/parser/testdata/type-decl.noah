pub type Vec []char

enum Color {
    Red,
    Green,
}

pub interface Person {
    say() -> string
}

pub struct Man {
    name: string
}

pub struct Woman {
    nick: string
}

impl (Person) Man {
    pub fn say() {
        return "Man: " + self.name
    }
}

impl (a.Person) b.Woman {
    pub fn say() {
        return "Woman: " + self.nick
    }
}

impl c.Woman {
    pub fn say() {
        return "Woman: " + self.nick
    }
}

struct OldMan <- Man {
    age: number
}

struct Student <- a.Man, b.Woman {
    scores: []number
}